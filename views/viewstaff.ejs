<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>

    <table border="">
        <tr>
            <th>Id:</th>
            <th>Name:</th>
            <th>email:</th>
            <th>password:</th>
            <th>std:</th>
            <th>div:</th>
        </tr>
        <% result.forEach(function(data){%>
            <tr>
                <td>
                    <%= data.id%>
                </td>
                <td>
                    <%=data.name%>
                </td>
                <td>
                    <%=data.email%>
                </td>
                <td>
                    <%=data.password%>
                </td>
                <td>
                    <%=data.std%>
                </td>
                <td>
                    <%=data.division%>
                </td>
            </tr>
            <% });%>

    </table>
</body>

</html>